{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://my-app-domain/schemas/booklist",
  "title": "BookList",
  "description": "Book List Schema Document",
  "type": "object",
  "required": [
    "id",
    "title",
    "author_last_name",
    "publication_date",
    "location"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "author_first_name": {
      "type": "string"
    },
    "author_last_name": {
      "type": "string"
    },
    "publication_date": {
      "type": "number"
    },
    "series_name": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "bookcase": {
      "type": "number"
    },
    "tracking": {
        "type": "object",
        "properties": {
            "uuid": {
            "type": "string",
            "format": "uuid"
        },
            "createdDate": {
                "type": "string"
        },
            "updatedDate": {
                "type": "string"
        }
        }
    }
    }
}